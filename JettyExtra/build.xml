targets:

all         - Build all sub packages
jmx         - Build the jmx package.
sasl        - Build the sasl package.
clean       - Remove generated files

-->
<project name="Jetty3Extra" default="all" basedir=".">

  <target name="jmx">
    <ant dir="jmx"/>
  </target>

  <target name="sasl">
    <ant dir="sasl"/>
  </target>

  <target name="jmxclean">
    <ant dir="jmx" target="clean"/>
  </target>

  <target name="saslclean">
    <ant dir="sasl" target="clean"/>
  </target>

  <target name="all" depends="jmx,sasl"/>
  <target name="clean" depends="jmxclean,saslclean"/>

</project>







